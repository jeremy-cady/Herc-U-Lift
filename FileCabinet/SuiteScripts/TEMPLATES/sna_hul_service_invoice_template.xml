        <td align="left" style="width: 50%; padding-left: 183px;">Payment Terms : ${invoiceDetails.terms}</td>
    </tr>
    <tr>
        <td align="left" style="width: 50%;">Contact : ${invoiceDetails.contactEntityid}</td>
        <td align="left" style="width: 50%; padding-left: 183px;">Shipment Method : ${invoiceDetails.shipmethod}</td>
    </tr>
    <tr>
        <td align="left" style="width: 50%;">Phone No. : ${invoiceDetails.contactPhone}</td>
        <td align="left" style="width: 50%;padding-left: 183px;">Salesrep : ${invoiceDetails.salesrep}</td>
    </tr>
    <tr>
        <td align="left" style="width: 50%;">Memo: ${invoiceDetails.memo}</td>
        <td align="left" style="width: 50%; padding-left: 183px;">Branch :
            ${invoiceDetails.locationHulCode}
        </td>
    </tr>
<!--    If Invoice > NXC Case > Equipment Asset info is available-->
<#if invoiceDetails.caseAssets?has_content>
