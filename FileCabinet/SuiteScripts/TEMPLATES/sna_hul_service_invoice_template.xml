<#assign totalorderr_= subtotaltotalorder_ + taxtotalorder_>
<tr>
    <td colspan="4">&nbsp;</td>
    <th align="right">
        <strong>Total This Invoice</strong>
    </th>
    <td align="right" style="border-bottom: solid #000000;">${totalorderr_?string.currency}</td>
</tr>
