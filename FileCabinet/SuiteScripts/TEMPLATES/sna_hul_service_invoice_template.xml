        <!-- start items/expenses -->
        <!-- <span class="number">${invoiceDetails.lines@label}</span>-->
<table style="width: 100%; margin-top: 10px;">
    <thead>
        <tr>
            <th align="left" style="border-bottom: solid #000000;border-top: solid #000000;">Item</th>
            <th align="left" style="border-bottom: solid #000000;border-top: solid #000000;width: 150px;">Desc</th>
            <th align="right" style="border-bottom: solid #000000;border-top: solid #000000;width: 50px;">Qty</th>
            <!--            <th align="center" style="border-bottom: solid #000000;border-top: solid #000000;">Qty Shpd</th>-->
            <!--            <th align="center" style="border-bottom: solid #000000;border-top: solid #000000;">BO</th>-->
            <th align="right" style="border-bottom: solid #000000;border-top: solid #000000;">EA</th>
            <th align="right" style="border-bottom: solid #000000;border-top: solid #000000;">Net</th>
        </tr>
    </thead>
    <#assign total_material = 0>
    <#assign total_order = 0>
    <#assign internalamt = 0 />
    <#assign internaltaxamt = 0 />
    <!-- start items -->

    <#list invoiceDetails.lines as line>
        <#if line.item =="Planned Maintenance">
            <#assign has_planned_maintenance = true>
            <#break>
        <#else>
            <#assign has_planned_maintenance = false>
        </#if>
    </#list>

    <!-- If Item list has Planned Maintenance -->
    <#if has_planned_maintenance>
        <#list invoiceDetails.lines as line>
            <#if line.custcol_sna_do_not_print == "false" && line.custcol_sna_default_rev_stream == "false">
                <tr>
                    <#if line.itemtype=="InvtPart">
                        <#assign total_material = total_material + line.amount?number >
                    <#else>
                        <#assign total_order = total_order + line.amount?number >
                    </#if>

                    <#if line.itemtype=="Service">
                        <#if line.item=="Planned Maintenance">
                            <td align="left">${line.item}</td>
                            <td align="left" width="150px">${line.description?replace('\n', '<br/>', 'r')}</td>
                            <td align="right" width="50px">${line.quantity}</td>
                            <td align="right">${line.rate}</td>
                            <td align="right">${line.amount}</td>
                        </#if>
                    <#else>
                        <td align="left">${line.item}</td>
                        <td align="left" width="150px">${line.description?replace('\n', '<br/>', 'r')}</td>
                        <td align="right" width="50px">${line.quantity}</td>
                        <td align="right">${line.rate}</td>
                        <td align="right">${line.amount}</td>
                    </#if>
                </tr>
            <#else>
                <#assign internalamt = internalamt + line.amount?number />
                <#assign internaltaxamt = internaltaxamt + line.custcol_ava_taxamount?number />
            </#if>
        </#list>
    <!--No Planned Maintenance in Item list -->
    <#else>
        <#assign service_items = ['*Service Labor - Resource', '*Travel Time', 'Bereavement', 'Jury Duty', 'Labor Double Time', 'Labor Overtime', 'Lunch', 'Paid Time Off', 'S', 'SDT', 'Sick Leave', 'SOT', 'Travel Double Time', 'Travel Overtime', 'Unpaid Time Off', 'Vacation Leave']>
        <#assign service_item_net = 0>

        <#list invoiceDetails.lines as line>
            <#if line.custcol_sna_do_not_print == "false" && line.custcol_sna_default_rev_stream == "false">
                <tr>
                    <#if line.itemtype=="InvtPart">
                        <#assign total_material = total_material + line.amount?number >
                    <#else>
                        <#assign total_order = total_order + line.amount?number >
                    </#if>

                    <#if line.itemtype == "Service">
                        <#list service_items as service_item>
                            <#if line.item == service_item>
                                <#assign included_service = true>
                                <#assign service_item_net = service_item_net + line.amount?number >
                                <#break>
                            <#else>
                                <#assign included_service = false>
                            </#if>
                        </#list>

                        <#if included_service == false>
                            <td align="left">${line.item}</td>
                            <td align="left" width="150px">${line.description?replace('\n', '<br/>', 'r')}</td>
                            <td align="right" width="50px"></td>
                            <td align="right">${line.rate}</td>
                            <td align="right">${line.amount}</td>
                        </#if>
                    <#else>
                        <td align="left">${line.item}</td>
                        <td align="left" width="150px">${line.description?replace('\n', '<br/>', 'r')}</td>
                        <td align="right" width="50px">${line.quantity}</td>
                        <td align="right">${line.rate}</td>
                        <td align="right">${line.amount}</td>
                    </#if>
                </tr>
            <#else>
                <#assign internalamt = internalamt + line.amount?number />
                <#assign internaltaxamt = internaltaxamt + line.custcol_ava_taxamount?number />
            </#if>
        </#list>

        <#if service_item_net != 0>
            <tr>
                <td align="left">Service Labor</td>
                <td align="left" width="150px"></td>
                <td align="right" width="50px"></td>
                <td align="right"></td>
                <td align="right">${service_item_net?string.currency}</td>
            </tr>
        </#if>
    </#if>
</table>

<table class="total" style="width: 100%; ">
<tr>
    <td colspan="4">&nbsp;</td>
    <th align="right">&nbsp;</th>
    <td align="right">&nbsp;</td>
</tr>
<tr>
