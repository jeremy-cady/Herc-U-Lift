  <#assign weekarray = [] />
  <#assign forweekarray = [] />

<#if record.item?has_content>

<table class='itemtable' style='width: 100%; margin-top: 10px;'>
  <!-- start items -->
  <#assign internalamt = 0 />
    <#assign internaltaxamt = 0 />

  <#list record.item as item><#if item_index==0>
<thead>
	<tr>
	<th>Make</th>
	<th>Model</th>
	<th>Desc</th>
	<th>Starting Date</th>
	<th>Finishing Date</th>
	<th align='right'>EA</th>
    <th align='right'>Net</th>
	</tr>
</thead>
</#if>
  <#if !item.custcol_sna_do_not_print && !item.custcol_sna_default_rev_stream>
  <tr>
	<td align='left'><#if item.item.internalid == '98427' || item.item == 'Rental Charge'>${item.cseg_hul_mfg}<#else></#if></td>
	<td align='left'><#if item.item.internalid == '98427' || item.item == 'Rental Charge'>${item.custcol_sna_hul_obj_model}<#else>${item.item}</#if></td>
    <td align='left'><#if item.item.internalid == '98427' || item.item == 'Rental Charge'><#else>${item.custcol_sna_sales_description}</#if></td>
	<td><#if item.item.internalid == '98427' || item.item == 'Rental Charge'>${item.custcol_sna_hul_rent_start_date}<#else></#if></td>
	<td><#if item.item.internalid == '98427' || item.item == 'Rental Charge'>${item.custcol_sna_hul_rent_end_date}<#else></#if></td>
    <td align='right'><#if item.custcol_sna_hul_gen_prodpost_grp == 'LDW' || item.item.internalid == '98427' || item.item == 'Rental Charge'><#else>${item.rate}</#if></td>
	<td align='right'>${item.amount}</td>
	</tr>



     <#if item.custcol_sna_rental_rate_card?has_content>

       <#assign rcarray = rcarray + ['${item.custcol_sna_hul_obj_model}'] />
   <#assign dayarray = dayarray + ['${item.custcol_sna_day_rate}'] />
       <#assign weekarray = weekarray + ['${item.custcol_sna_weekly_rate}'] />
        <#assign forweekarray = forweekarray + ['${item.custcol_sna_4week_rate}'] />


    </#if>

  <#else>
       <#assign internalamt = internalamt + item.amount />
       <#assign internaltaxamt = internaltaxamt + item.custcol_ava_taxamount />
    </#if>

	</#list><!-- end items --></table>

</#if>
<table class='total' style='width: 100%; margin-top: 10px;'>
  <#assign subtotaltotalorder_= record.subtotal - internalamt>
        <#assign taxtotalorder_= record.taxtotal - internaltaxamt>
   <#assign totalorderr_= subtotaltotalorder_ + taxtotalorder_>

  <tr>
	<td colspan='4'>&nbsp;</td>
	<td align='right'><b>${record.subtotal@label}</b></td>
	<!-- <td align='right'>${record.subtotal}</td> -->
    <td align='right'>${subtotaltotalorder_?string.currency}</td>
	</tr>
	<tr>
