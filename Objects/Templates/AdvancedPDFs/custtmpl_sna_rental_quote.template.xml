   <#if dayarray[i]?has_content>       <#assign day = dayarray[i]> <#else><#assign day = ''></#if>
     <#if weekarray[i]?has_content>   <#assign week = weekarray[i]><#else><#assign week = ''></#if>
    <#if forweekarray[i]?has_content>    <#assign month = forweekarray[i]><#else><#assign month = ''></#if>

         <tr>
      <td>${rcarray[i]}</td>
      <td>Day</td>
      <td>${day}</td>
      <td>Week</td>
      <td>${week}</td>
      <td>4 weeks</td>
      <td>${month}</td>
    </tr>


</#list>
