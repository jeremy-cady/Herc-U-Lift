<body header="nlheader" header-height="38%" footer="nlfooter" footer-height="10%" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
    <!-- start transaction header --><!-- end transaction header --><!-- start items/expenses --><!-- <span class="number">${record.item@label}</span> -->
<table style="width: 100%; margin-top: 10px;">
<thead>
	<tr>
	<th align="left" style="border-bottom: solid #000000;border-top: solid #000000;">${record.item[0].item@label}</th>
    	<th align="center" style="border-bottom: solid #000000;border-top: solid #000000;width: 50px;">Quantity</th>
	<th align="center" style="border-bottom: solid #000000;border-top: solid #000000;width: 300px;">Description</th>
	<!-- <th align="center" style="border-bottom: solid #000000;border-top: solid #000000;">UOM</th> -->
	<th align="center" style="border-bottom: solid #000000;border-top: solid #000000;">Rate</th>
	<th align="center" style="border-bottom: solid #000000;border-top: solid #000000;">Amount</th>
	</tr>
</thead>
<!-- start items -->
  <#assign internalamt = 0 />
  <#assign internaltaxamt = 0 />
  
  <#list record.item as item>
    <#if !item.custcol_sna_do_not_print && !item.custcol_sna_default_rev_stream> 
    <tr>
	<td align="left">${item.item}</td>
    	<td align="center">${item.quantity}</td>
	<td align="center">${item.description}</td>
	<!-- <td align="center">${item.uom}</td> -->
	<td align="center">${item.rate}</td>
	<td align="center">${item.amount}</td>
	</tr>
       <#else>
            <#assign internalamt = internalamt + item.amount />
            <#assign internaltaxamt = internaltaxamt + item.custcol_ava_taxamount />
          </#if>
	</#list></table>

<table class="total" style="width: 100%; "><tr>
	<td colspan="4">&nbsp;</td>
	<th align="right">&nbsp;</th>
	<td align="right">&nbsp;</td>
	</tr>
  <#assign subtotaltotalorder_= record.subtotal - internalamt>
        <#assign taxtotalorder_= record.taxtotal - internaltaxamt>
	<!-- <tr>
	<td colspan="4">&nbsp;</td>
	<th align="right"><strong>Total</strong></th>
       <td align="right">${subtotaltotalorder_?string.currency}</td> 
	</tr> -->
	<tr>
	<td colspan="4">&nbsp;</td>
	<th align="right"> <strong>${record.subtotal@label} </strong></th>
       <td align="right" style="padding-right:8px;">&nbsp;${subtotaltotalorder_?string.currency}</td> 
      
	</tr>
	<tr>
	<td colspan="4">&nbsp;</td>
	<th align="right" style="text"> <strong>Tax Total</strong></th>
      <td align="right" style="padding-right:8px;">${taxtotalorder_?string.currency}&nbsp;</td>
         

	</tr>
          <#assign totalorderr_= subtotaltotalorder_ + taxtotalorder_>
	<tr>
	<td colspan="4">&nbsp;</td>
	<th align="right"><strong>Total </strong></th>
	<td align="right" style="padding-right:8px;">${totalorderr_?string.currency}&nbsp;</td>
   
	</tr></table>
